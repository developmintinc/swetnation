<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="Site.master.cs" Inherits="SwetNation.Web.Site" %>
<%@ Register Src="~/controls/HeaderStyles.ascx" TagName="HeaderStyles" TagPrefix="uc" %>
<%@ Register Src="~/controls/HeaderScripts.ascx" TagName="HeaderScripts" TagPrefix="uc" %>
<%@ Register Src="~/controls/HeaderFonts.ascx" TagName="HeaderFonts" TagPrefix="uc" %>
<%@ Register Src="~/controls/SearchBox.ascx" TagName="SearchBox" TagPrefix="uc" %>
<%@ Register Src="~/controls/SocialIcons.ascx" TagName="SocialIcons" TagPrefix="uc" %>
<%@ Register Src="~/controls/PrimaryNav.ascx" TagName="PrimaryNav" TagPrefix="uc" %>
<%@ Register Src="~/controls/FooterInformation.ascx" TagName="FooterInformation" TagPrefix="uc" %>
<%@ Register Src="~/controls/FooterNav.ascx" TagName="FooterNav" TagPrefix="uc" %>
<html lang="en">
<head id="Head1" runat="server">
    <title>Swet Nation - Huge discount on top sports apparel</title>
    <asp:ContentPlaceHolder ID="head" runat="server">
    </asp:ContentPlaceHolder>
    <!--META STARTS-->
    <meta name="description" content="SwetNation - SNOW + SWEAT + SKI + ROCK"/>
    <meta name="keywords" content=""/>
    <meta name="author" content="SecondStrategy.com"/>
    <!--META ENDS-->
    <link rel="shortcut icon" type="image/gif" href="images/favicon.gif"/>
    <!--CSS FILES STARTS-->
    <uc:HeaderStyles ID="ucHeaderStyles" runat="server" />
    <!--CSS FILES ENDS-->
    <!--CHOOSE WICH FONT YOU WANT TO USE AND DELETE THE REST FOR FASTER LOADING-->
    <!--GOOGLE FONTS STARTS-->
    <uc:HeaderFonts ID="ucHeaderFonts" runat="server" />
    <!--GOOGLE FONTS ENDS-->
    <!--JS FILES STARTS-->
    <uc:HeaderScripts ID="ucHeaderScripts" runat="server" />
    <!--JS FILES ENDS-->
</head>
<body>
    <form id="form1" runat="server">
    <div id="wrapper">
	    <a href="./m" class="find-us"></a>
	    <div class="center-page">
		    <div id="container">
			    <div id="header-intro-panel">
				    <div class="center">
				        <!-- SITE SEARCH STARTS -->
					    <uc:SearchBox ID="ucSearchBox" runat="server" />
                        <!-- SITE SEARCH ENDS -->
                        <!-- SOCIAL NETWORKS ICONS STARTS -->
				        <uc:SocialIcons ID="ucSocialIcons" runat="server" />
                        <!-- SOCIAL NETWORKS ICONS ENDS -->
				    </div>
			    </div>
                <!-- HEADER CONTAINER STARTS -->
			    <div id="header-container">
				    <div class="header-center">
                        <!-- HEADER STARTS -->
					    <div id="header">
                            <!-- LOGO STARTS -->
						    <a id="logo" title="Homepage" href="Default.aspx">Homepage</a>
						    <!--LOGO ENDS HERE  -->
						    <!-- PRIMARY NAV STARTS -->
					        <uc:PrimaryNav ID="ucPrimaryNav" runat="server" />
                            <!-- PRIMARY NAV ENDS  -->
					    </div>
					    <!-- HEADER ENDS-->
				    </div>
			    </div>
                <!-- HEADER CONTAINER ENDS -->
			    <div class="post-header-shadow">
			    </div>
                <!-- MAIN CONTENT STARTS-->
                <asp:ContentPlaceHolder ID="SiteContentPlaceHolder" runat="server">
                </asp:ContentPlaceHolder>
                <!-- MAIN CONTENT ENDS-->
			    <div class="pre-footer-shadow">
			    </div>
			    <!-- FOOTER INFORMATION STARTS-->
		        <uc:FooterInformation ID="ucFooterInformation" runat="server" />
                <!-- FOOTER INFORMATION ENDS-->
			    <!-- FOOTER NAV STARTS-->
                <uc:FooterNav ID="ucFooterNav" runat="server" />
                <!-- FOOTER NAV ENDS-->
		    </div>
	    </div>
    </div>        
    </form>
</body>
</html>
